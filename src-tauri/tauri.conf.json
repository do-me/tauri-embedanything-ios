{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "Offline OSM",
  "version": "0.1.0",
  "identifier": "app.somethin.ggggg",
  "build": {
    "beforeDevCommand": "deno run -A build.ts dev",
    "beforeBuildCommand": "deno run -A build.ts build",
    "devUrl": "http://0.0.0.0:3000",
    "frontendDist": "../src-www"
  },
  "app": {
    "windows": [
      {
        "title": "Offline OSM",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": ["$APPDATA/**"]
        }
      },
      "csp": null,
      "dangerousDisableAssetCspModification": false,
      "freezePrototype": false,
      "pattern": {
        "use": "brownfield"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
